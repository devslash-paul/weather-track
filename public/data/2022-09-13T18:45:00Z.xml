            </forecast-period>
            <forecast-period index="5" start-time-local="2022-09-19T00:00:00+10:00" end-time-local="2022-09-20T00:00:00+10:00" start-time-utc="2022-09-18T14:00:00Z" end-time-utc="2022-09-19T14:00:00Z">
