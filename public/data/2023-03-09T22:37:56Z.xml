            </forecast-period>
            <forecast-period index="5" start-time-local="2023-03-15T00:00:00+11:00" end-time-local="2023-03-16T00:00:00+11:00" start-time-utc="2023-03-14T13:00:00Z" end-time-utc="2023-03-15T13:00:00Z">
