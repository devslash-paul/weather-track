            <forecast-period index="1" start-time-local="2022-12-15T00:00:00+11:00" end-time-local="2022-12-16T00:00:00+11:00" start-time-utc="2022-12-14T13:00:00Z" end-time-utc="2022-12-15T13:00:00Z">
