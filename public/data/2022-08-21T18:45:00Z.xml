            <forecast-period index="6" start-time-local="2022-08-28T00:00:00+10:00" end-time-local="2022-08-29T00:00:00+10:00" start-time-utc="2022-08-27T14:00:00Z" end-time-utc="2022-08-28T14:00:00Z">
